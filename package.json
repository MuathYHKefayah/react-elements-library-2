{
  "name": "@MuathYHKefayah/react-elements-library-tsdx",
  "version": "0.1.0",
  "description": "Made with tsdx",
  "license": "MIT",
  "repository": "https://github.com/MuathYHKefayah/react-elements-library-tsdx.git",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "sb dev",
    "build-storybook": "sb build"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "react-elements-library-tsdx",
  "author": "Moath Kefayah",
  "module": "dist/react-elements-library-tsdx.esm.js",
  "size-limit": [
    {
      "path": "dist/react-elements-library-tsdx.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-elements-library-tsdx.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@size-limit/preset-small-lib": "^9.0.0",
    "@storybook/addon-essentials": "^7.4.6",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^7.4.6",
    "@storybook/addons": "^7.4.6",
    "@storybook/react": "^7.4.6",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "babel-loader": "^9.1.3",
    "husky": "^8.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "size-limit": "^9.0.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
